public class MyExample4 {
    public static void main(String[] args) {
	      Calculator obj = new Calculator();
	      System.out.println(obj.num1 - obj.num2);
	      System.out.println(obj.num1 + obj.num2);
	      System.out.println(obj.num1 * obj.num2);
	      System.out.println(obj.num1 / obj.num2);
    }
}
