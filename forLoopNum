public class HelloWorld {
  
   public static void main(String args[]){
     int myNumber = 3;

     if( myNumber == 3 ){
        for(int i = 1; i <= 5; i++){
          System.out.println(i*3);
      }
    }
 
    else if ( myNumber == 7 ){
        for(int repeat = 1; repeat <= 5; repeat++){
          System.out.println(repeat*7);
      }
    }
    
  }
}
